{
	"name": "@karma-lies/reset",
	"version": "1.0.1",
	"engines": {
		"node": ">=14.13",
		"npm": ">=6.14"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"netlify": "netlify dev",
		"dev": "svelte-kit dev --host",
		"prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview --https",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@poppanator/sveltekit-svg": "0.2.2",
		"@sendgrid/mail": "7.5.0",
		"@sveltejs/adapter-static": "1.0.0-next.34",
		"@sveltejs/kit": "1.0.0-next.347",
		"@vimeo/player": "2.16.1",
		"autoprefixer": "10.4.7",
		"axios": "0.27.2",
		"cssnano": "5.1.10",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-svelte3": "3.2.1",
		"graphql": "15.6.1",
		"graphql-request": "3.6.0",
		"netlify-cli": "6.9.30",
		"postcss": "8.4.14",
		"postcss-load-config": "4.0.1",
		"prettier": "2.5.1",
		"prettier-plugin-svelte": "2.4.0",
		"svelte": "3.48.0",
		"svelte-preprocess": "4.10.7",
		"tailwindcss": "3.0.24"
	},
	"type": "module"
}
